{
  "name": "minidrone-js",
  "version": "0.0.1",
  "description": "Parrot Minidrone library",
  "main": "dist/bundle.min.js",
  "repository": "https://github.com/Mechazawa/minidrone-js",
  "author": "Bas <mega@ioexception.at>",
  "license": "MIT",
  "dependencies": {
    "arsdk-xml": "git://github.com/Parrot-Developers/arsdk-xml",
    "events": "^1.1.1",
    "noble": "^1.8.1",
    "node-gyp": "^3.6.2",
    "winston": "^2.4.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.16.0",
    "fs-extra": "^5.0.0",
    "fsevents": "^1.1.3",
    "path": "^0.12.7",
