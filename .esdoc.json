{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "accessor": {"access": ["public", "protected"], "autoPrivate": true},
        "lint": {"enable": true},
        "typeInference": {"enable": true}
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
